		<% if (variable.show !== false && variable.legend !== false && variable.enabled !== false) { %>
		<li>
		<span class="legend-swatch" style="background-color: <%= variable.legendFillColor || variable.color || variable.options.color %>; border-color: <%= variable.legendBorderColor || variable.color || variable.options.color %>;"></span>
		<%
					var units = '';
					var legendUnits = variable.legendUnits;
					if (legendUnits === false) {
						// no units shown in the legend label
					} else if (_.isString(legendUnits)) {
						units = $.trim(legendUnits);
					} else {
						units = $.trim(variable.units);
					}
				%>
		<span class="legend-label">
		<%= variable.options.label %>
		<% if (units) { %> (<%= units %>)<% } %>
		</span>
		</li>
		<% } %>
	<% }); %>
	</ul>
