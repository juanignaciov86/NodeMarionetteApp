		<tr>
		<td><%= wui.date.strftime("%l %P",hour.date) %></td>
		<td class="nobr"><%= hour.temperature %> <%= labels.temperature.units %></td>
		<td class="nobr"><%= hour.feelslike %> <%= labels.feelslike.units %></td>
		<td class="nobr"><%= hour.dewpoint %> <%= labels.dewpoint.units %></td>
		<td><%= hour.humidity %><%= labels.humidity.units %></td>
		<td><img src="<%= hour.icon_url %>" alt="<%= hour.condition %>" class="no-scale" /></td>
		<td><%= hour.condition %></td>
		<td><% if (index < 6) { %><a href="<%= cityUrl %>/precipitation" class="precip-link-table"><% } %><%= hour.pop %><%= labels.pop.units %><% if (index < 6) { %></a><% } %></td>
		<td>
		<% if (index < 6) { %><a href="<%= cityUrl %>/precipitation" class="precip-link-table"><% } %>
		<% if (hour.precip_type === 'snow') { %>
		<%
												var defaultSnow = weather.response.units === 'metric' ? '2' : '1';
												var hourSnow = Math.round(hour.snow);

												if (hourSnow >= defaultSnow) {
		hourSnow = '~ ' + hourSnow;
		} else {
		hourSnow = '< ' + defaultSnow;
												}
											%>
		<%= hourSnow %> <%= labels.snow_depth.units %>
		<% } else { %>
		<%= hour.liquid_precip %> <%= labels.rainfall.units %>
		<% } %>
		<% if (index < 6) { %></a><% } %>
		</td>
		<td><span class="nobr"><%= hour.wind_speed %></span> <%= hour.wind_dir %></td>
		<td><%= hour.cloudcover %><%= labels.cloudcover.units %></td>
		<td><%= hour.pressure %><%= labels.pressure.units %></td>
		</tr>
		<% }); %>
		</tbody>
		</table>
		</div>
		</dd>
	<% } %>
</script>
