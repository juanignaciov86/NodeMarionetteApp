<script type="text/template" id="table-vertical-template">
	<% for (var index = 0; index < weather.forecast.days.length; index++) { %>
		<% var day = weather.forecast.days[index]; %>
		<% var astronomyDay = weather.astronomy && weather.astronomy.days.length ? weather.astronomy.days[index] : null; %>
		<% var latitude = weather.response && weather.response.location && typeof weather.response.location.latitude !== 'undefined' ? weather.response.location.latitude : null; %>
		<% var moonDirection = 'moonNorth'; %>
		<% var errorString = 'Not Available'; %>
		<% var current_observation = weather.current_observation; %>
		<% var station = current_observation.station; %>
		<dd id="fctDay-<%= day.summary.date.year %><%= wui.date.pad(day.summary.date.month) %><%= wui.date.pad(day.summary.date.day) %>" class="accordion-navigation <% if (index === 0) { %>active<% } %>">
		<a href="#vertical-day-<%= day.summary.date.yday %>">
		<span class="date"><span class="b"><%= day.summary.date.weekday_short %></span> <%= day.summary.date.month %>/<%= day.summary.date.day %></span>
		<span class="temps"><span class="b"><%= day.summary.high %></span><%= labels.temperature.units %> | <%= day.summary.low %><%= labels.temperature.units %></span>
		<span class="conds"><img src="<%= day.summary.icon_url %>" alt="<%= day.summary.condition %>" /><span class="conds-label"><%= day.summary.condition %></span></span>
		<span class="pop<% if (day.summary.day.precip_type === 'snow') {%> snow<% } if(day.summary.pop >= 30) {%> wet<%}%>"><span title="<%= labels.pop.label %>"></span><%= day.summary.pop %><%= labels.pop.units %></span>
		<span class="arrow"></span>
		</a>
		<div id="vertical-day-<%= day.summary.date.yday %>" class="content<% if (index === 0) { %> active<% } %>">
