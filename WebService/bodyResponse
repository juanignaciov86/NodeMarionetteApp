<script type="text/template" id="station-list-template">
	<%
		var wind_chill = false;
		var heat_index = false;
		var valueOrDash = function(value) {
			return ($.isNumeric(value) ? value : '--');
		}
		_.each(nearbyStations, function(station) {
			var pws = station;
			if(pws.windchill)
				wind_chill = true;
			if(pws.heatindex)
				heat_index = true;
		});
	%>
	<thead>
		<tr>
		<th scope="col" data-tablesaw-sortable-col data-tablesaw-sortable-default-col data-tablesaw-priority="persist">Station Location</th>
		<th scope="col" data-tablesaw-sortable-col data-tablesaw-priority="persist">Temp.</th>
		<% if(wind_chill) { %>
		<th scope="col" data-tablesaw-sortable-col data-tablesaw-priority="1">Windchill</th>
		<% } %>
		<% if(heat_index) { %>
		<th scope="col" data-tablesaw-sortable-col data-tablesaw-priority="1">Heat Index</th>
		<% } %>
		<th scope="col" data-tablesaw-sortable-col data-tablesaw-priority="2">Dew Point</th>
		<th scope="col" data-tablesaw-sortable-col data-tablesaw-priority="3">Humidity</th>
		<th scope="col" data-tablesaw-sortable-col data-tablesaw-priority="4">Wind</th>
		<th scope="col" data-tablesaw-sortable-col data-tablesaw-priority="5">Precip.</th>
		<th scope="col" data-tablesaw-sortable-col data-tablesaw-priority="6">Elev</th>
		<th scope="col" data-tablesaw-sortable-col data-tablesaw-priority="6">Updated</th>
		<th scope="col" data-tablesaw-sortable-col data-tablesaw-priority="6">Type</th>
		</tr>
	</thead>
	<tbody>
	<% if(nearbyStations.length ==0) { %>
		<tr>
		<td colspan="9">There are no personal weather stations in this area.</td>
		</tr>
	<% } %>
	<%
	_.each(nearbyStations, function(station) {
		var pws = station;
	%>
		<tr>
		<td tablesaw-cell-persist>
		<a href="/weatherstation/WXDailyHistory.asp?ID=<%= pws.id %>" title="Weather in <%= pws._title %>"><%= pws._title %></a>
		</td>
		<td tablesaw-cell-persist>
		<span class="pwsrt" pwsid="<%= pws.id %>" pwsvariable="temperature">
		<span class="wx-data">
		<span class="wx-value">
		<%= valueOrDash(pws.temperature) %>
		</span>
		<span class="wx-unit">
		<%= labels.temperature.units %>
		</span>
		</span>
		</span>
		</td>
		<% if (wind_chill) { %>
		<td>
		<span class="pwsrt" pwsid="<%= pws.id %>" pwsvariable="windchill">
		<span class="wx-data">
		<span class="wx-value">
		<%= valueOrDash(pws.windchill) %>
		</span>
		<span class="wx-unit">
		<%= labels.temperature.units %>
		</span>
		</span>
		</span>
		</td>
		<% } %>
		<% if (heat_index) { %>
		<td>
		<span class="pwsrt" pwsid="<%= pws.id %>" pwsvariable="heatindex">
		<span class="wx-data">
		<span class="wx-value">
		<%= valueOrDash(pws.heatindex) %>
		</span>
		<span class="wx-unit">
		<%= labels.temperature.units %>
		</span>
		</span>
		</span>
		</td>
		<% } %>
		<td>
		<span class="pwsrt" pwsid="<%= pws.id %>" pwsvariable="dewpoint">
		<span class="wx-data">
		<span class="wx-value">
		<%= valueOrDash(pws.dewpoint) %>
		</span>
		<span class="wx-unit">
		<%= labels.dewpoint.units %>
		</span>
		</span>
		</span>
		</td>
		<td>
		<span class="pwsrt" pwsid="<%= pws.id %>" pwsvariable="humidity">
		<span class="wx-data">
		<span class="wx-value"><%= valueOrDash(pws.humidity) %></span><span class="wx-unit"><%= labels.humidity.units %></span>
		</span>
		</span>
		</td>
		<td>
		<span class="pwsrt" pwsid="<%= pws.id %>" pwsvariable="wind_direction">
		<%= wx.lookupWindString(pws.wind_dir_degrees) %>
		</span>
		<span>&nbsp;<%= labels.wind_speed.separator %>&nbsp;</span>
		<span class="pwsrt" pwsid="<%= pws.id %>" pwsvariable="wind_speed">
		<span class="wx-data">
		<span class="wx-value">
		<%= valueOrDash(parseFloat(pws.wind_speed).toFixed(1)) %>
		</span>
		<span class="wx-unit">
		<%= labels.wind_speed.units %>
		</span>
		</span>
		</span>
		</td>
		<td>
		<span class="pwsrt" pwsid="<%= pws.id %>" pwsvariable="precip_rate">
		<span class="wx-data">
		<span class="wx-value">
		<%= valueOrDash(parseFloat(pws.precip_rate).toFixed(2)) %>
		</span>
		<%= labels.rainfall.units %> / hr
		</span>
		</span>
		</td>
		<td class="nobr">
		<span class="wx-data">
		<span class="wx-value">
		<%= pws.elevation %>
		</span>
		<span class="wx-unit">
		<%= labels.elevation.units %>
		</span>
		</span>
		</span>
		</td>
		<td>
		<% var d = new Date(pws.updated * 1000);
					var offset = weather.response.date.tz_offset_hours;
					var timezone = weather.response.date.tz_short;
					wui.date.increment(d, offset, 'hour');
					var date_string = wui.date.strftime('%l:%M %p', d, true) + ' ' + timezone;
				%>
		<span class="pwsrt"><%= date_string %></span>
		</td>
		<td>
		<% if(pws.rtfreq && pws.rtfreq < 60) %>
		<span class="green">Rapid Fire</span>
		<% else %>
		Normal
		</td>
		</tr>
	<% }); %>
	</tbody>
</script>
