		<div class="columns hide-for-medium-up">
		<div class="precip-message">
		<div class="precip-icon">
		<% if (precip.eventType !== 'noprecip') { %><img src="//icons.wxug.com/i/o/precip/light<%= precip.eventType %>.svg" alt="<%= precip.eventType %>" width="50" height="50"><% } %>
		</div>
		<p><a href="<%= cityUrl %>/precipitation" class="precip-link-table"><span class="statement-text"><%= precip.statement %></span></a></p>
		</div>
		</div>
		<% } %>
		<div class="fctText row astronomy-data">
		<div class="fct-item sun small-4 columns">
		<div class="fct-content">
		<h5>Sun</h5>
		<span class="rise">
		<span class="astro-data sunrise-icon">
		<% if (astronomyDay.sunrise.date.hour !== null) { %>
		<%= wui.date.strftime("%l:%M", astronomyDay.sunrise.date) %>
		<% } else { %>
		<%= errorString %>
		<% } %>
		</span>
		<span class="ampm">
		<% if (astronomyDay.sunrise.date.hour !== null) { %>
		<%= wui.date.strftime("%p", astronomyDay.sunrise.date) %>
		<% } %>
		</span>
		</span>
		<br />
		<span class="set">
		<span class="astro-data sunset-icon">
		<% if (astronomyDay.sunset.date.hour !== null) { %>
		<%= wui.date.strftime("%l:%M", astronomyDay.sunset.date) %>
		<% } else { %>
		<%= errorString %>
		<% } %>
		</span>
		<span class="ampm">
		<% if (astronomyDay.sunset.date.hour !== null) { %>
		<%= wui.date.strftime("%p", astronomyDay.sunset.date) %>
		<% } %>
		</span>
		</span>
		</div>
		</div>
		<div class="fct-item moon small-4 columns">
		<div class="fct-content">
		<h5>Moon</h5>
		<% if (latitude !== null) { %>
